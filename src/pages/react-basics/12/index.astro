---
import Layout from '#layouts/Layout.astro';
import {getCollection} from "astro:content";

const collection = await getCollection("react_basics_12");
---

<Layout>
    <main>
	<h2>React basics</h2>
    <p>
        (out of date) React Basics for v0.12
    </p>
	<ol>
		{collection.map(({data: {order, snippet: video}}) => {
			return (
				<li>
                    <a href={`/react-basics/12/${order}`}>
						{video.title}
					</a>
				</li>
			)
		})}
	</ol>
    </main>
</Layout>

